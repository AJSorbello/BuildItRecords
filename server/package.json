{
  "name": "builditrecords-server",
  "version": "1.0.0",
  "private": true,
  "description": "Server for BuildIt Records",
  "main": "server.js",
  "engines": {
    "node": "22.x"
  },
  "license": "MIT",
  "overrides": {
    "pg-native": "npm:@vercel/noop",
    "libpq": "npm:@vercel/noop"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon",
    "build": "tsc",
    "deploy": "npm install --production",
    "lint": "eslint . --ext .ts",
    "type-check": "tsc --noEmit",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "db:seed:all": "sequelize-cli db:seed:all",
    "db:seed:undo:all": "sequelize-cli db:seed:undo:all"
  },
  "dependencies": {
    "@types/nodemailer": "^6.4.17",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.16",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.5",
    "spotify-web-api-node": "^5.0.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "4.9.5",
    "uuid": "^11.0.4",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.13.4",
    "@types/pg": "^8.11.0",
    "@types/spotify-web-api-node": "^5.0.11",
    "@types/swagger-jsdoc": "^6",
    "nodemon": "^3.0.2",
    "pnpm": "^7.15.1",
    "sequelize-cli": "^6.6.2"
  }
}
