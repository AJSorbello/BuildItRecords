{
  "name": "builditrecords",
  "version": "1.0.0",
  "description": "Build It Records - Music Label Website",
  "main": "index.js",
  "private": true,
  "proxy": "http://localhost:3001",
  "workspaces": [
    "server"
  ],
  "scripts": {
    "start": "react-scripts start",
    "server": "node server/server.js",
    "server:dev": "nodemon server/server.js",
    "dev": "concurrently \"yarn server:dev\" \"yarn start\"",
    "db:init": "node -e \"require('./server/db/init.js')().catch(console.error)\"",
    "test": "jest",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "db:start": "pg_ctl -D /usr/local/var/postgres start",
    "db:stop": "pg_ctl -D /usr/local/var/postgres stop",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc",
    "vercel-build": "yarn build"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.3",
    "@mui/material": "^5.15.3",
    "@mui/system": "^5.15.3",
    "@mui/x-date-pickers": "^6.18.6",
    "@spotify/web-api-ts-sdk": "^1.1.2",
    "@testing-library/jest-dom": "^6.1.6",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/pg": "^8.11.10",
    "@types/spotify-web-api-node": "^5.0.11",
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "date-fns": "^3.0.6",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "framer-motion": "^10.17.9",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.3.0",
    "react-intersection-observer": "^9.13.1",
    "react-router-dom": "^7.0.2",
    "react-scripts": "5.0.1",
    "sequelize": "^6.35.2",
    "spotify-web-api-node": "^5.0.2",
    "ts-node": "^10.9.2",
    "typescript": "4.9.5",
    "web-vitals": "^3.5.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "cookie": "^1.0.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "source-map-loader": "^5.0.0",
    "supertest": "^6.3.3",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "author": "",
  "license": "ISC"
}
