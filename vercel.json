{
  "version": 2,
  "buildCommand": "rm -f pnpm-lock.yaml && node ./scripts/ci-mode-patch.js && node ./scripts/fix-package-json.js && node ./scripts/bypass-vercel-check.js && node ./scripts/vercel-build-patch.js && pnpm install --force && pnpm run build",
  "installCommand": "rm -f pnpm-lock.yaml && node ./scripts/ci-mode-patch.js && pnpm install --force",
  "ignoreCommand": "",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "zeroConfig": true,
        "skipInstall": false
      }
    },
    {
      "src": "server/server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["server/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/server.js"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|svg|ico|json))",
      "dest": "/dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    }
  ],
  "git": {
    "deploymentEnabled": {
      "main": true,
      "vercel-deploy-fix": true
    }
  },
  "github": {
    "enabled": true,
    "silent": false,
    "autoJobCancelation": true
  }
}
